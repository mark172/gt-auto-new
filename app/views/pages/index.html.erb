                  <div id="intro-pager" class="tab-pager">
                    <a data-slide-index="0" href="#"><i class="icon fa fa-arrow-right"></i><span>Free Quote</span></a>
                    <a data-slide-index="1" href="#"><i class="icon fa fa-arrow-right"></i><span>place an order</span></a>
                    <a data-slide-index="2" href="#"><i class="icon fa fa-arrow-right"></i><span>track shipment</span></a>
                    <a data-slide-index="3" href="#"><i class="icon fa fa-arrow-right"></i><span>contact us</span></a>
                  </div>
                  <!-- /Pager -->
                </div>
              </div>
            </div>
            <!-- /Col -->
            
          </div>
          <!-- /Row -->
        </div>
        <!-- /Container -->
      
